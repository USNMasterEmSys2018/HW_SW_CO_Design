/*
 * Empty C++ Application
 */
#include <stdio.h>
#include "platform.h"
#include "xil_printf.h"
#include "LEDs.h"
#include "Buzzer.h"

int main()
{
    init_platform();

    printf("Hello World\n\r");
    LEDs LED;
    LED.LightUp('A');
    Buzzer buz;
    buz.Sing(440)
    cleanup_platform();
    return 0;
}
